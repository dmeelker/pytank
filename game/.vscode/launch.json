    "version": "0.2.0",
    "configurations": [
        


        {
            "name": "Run Game",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Run Editor",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/editor.py",
            "args": ["levels/level6.txt"],
            "console": "integratedTerminal"
        },
        {
            "name": "Run test",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/tests/tankmovertest.py",
            "args": [],
            "console": "integratedTerminal"
        }
    ]
}